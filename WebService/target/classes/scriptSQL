#CREATE DATABASE filmare_db_1;


---------------- SCRIPT POUR LA TABLE USERS --------------------



DROP TABLE if exists users;
DROP TABLE if exists videos;


create table users (
	idUsers INT,
	username VARCHAR(50),
	password VARCHAR(50),
	name VARCHAR(50),
	age INT,
	gender VARCHAR(50)
);
